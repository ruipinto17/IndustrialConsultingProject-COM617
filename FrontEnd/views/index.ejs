  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="../css/style.css" />
  </head>

  <body>
    <form id="form" method="POST">
      <div class="labels">
        <label for="TargetUrl">Target URL</label>
      </div>
      <input
        type="text"
        class="form-control"
        name="TargetUrl"
        placeholder="www.placeholder.com"
        value=""
      />
      <div class="labels">
        <label for="ThreadPool">Thread pool</label>
      </div>
      <input
        type="number"
        class="form-control"
        name="ThreadPool"
        placeholder="5"
        value=""
      />
      <div class="labels">
        <label for="RampUpTime">RampUp Time</label>
      </div>
      <input
        type="number"
        class="form-control"
        name="RampUpTime"
        placeholder="5"
        value=""
      />
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

    <div class="top-row">
